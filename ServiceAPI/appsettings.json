{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Msg": "{msg}",
  "ConsulSetting": {
    "ServiceName": "APIService",
    "ServiceIP": "127.0.0.1",
    "ServicePort": "9050",
    "ServiceHealthCheck": "/api/ApiMonitor/HealthCheck",
    "ConsulAddress": "http://localhost:8500"
  }
}
